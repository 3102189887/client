.update(this.entityForm).then(() => {\n          this.finishSave();\n        });\n      }\n    },\n    finishSave() {\n      this.$message.success(\"成功\");\n      this.getPage(this.pageIndex);\n      this.editing = false;\n    },\n    deleteItem(id) {\n      api.deleteItem(id).then(() => {\n        this.$message.success(\"删除成功\");\n        this.getPage(this.pageIndex);\n      });\n    }\n  },\n  created() {\n    this.query();\n  }\n};\n</script>\n\n<style scoped></style>\n"],"mappings":"AAiFA,YAAAA,GAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;QACAF,IAAA;MACA;MACAG,UAAA;MACAC,SAAA;MACAC,QAAA,EAAAN,GAAA,CAAAM,QAAA;MACAC,SAAA;MACAC,SAAA;MACAC,OAAA;IACA;EACA;EACAC,OAAA;IACAC,MAAA;MACAX,GAAA,CAAAY,YAAA,MAAAT,SAAA,CAAAF,IAAA,EAAAY,IAAA,CAAAC,GAAA;QACA,KAAAP,SAAA,GAAAO,GAAA;QACA,KAAAN,SAAA;QACA,KAAAO,OAAA;MACA;IACA;IACAA,QAAAP,SAAA;MACAR,GAAA,CAAAe,OAAA,CAAAP,SAAA,OAAAL,SAAA,CAAAF,IAAA,EAAAY,IAAA,CAAAC,GAAA;QACA,KAAAT,SAAA,GAAAS,GAAA;MACA;IACA;IACAE,OAAA;MACA,KAAAZ,UAAA;QACAa,EAAA;QACAhB,IAAA;MACA;MACA,KAAAQ,OAAA;IACA;IACAS,KAAAD,EAAA;MACAjB,GAAA,CAAAmB,GAAA,CAAAF,EAAA,EAAAJ,IAAA,CAAAC,GAAA;QACA,KAAAV,UAAA,GAAAU,GAAA;QACA,KAAAL,OAAA;MACA;IACA;IACAW,KAAA;MACA,SAAAhB,UAAA,CAAAa,EAAA;QACAjB,GAAA,CAAAgB,MAAA,MAAAZ,UAAA,EAAAS,IAAA;UACA,KAAAQ,UAAA;QACA;MACA;QACArB,GAAA,CAAAsB,MAAA,MAAAlB,UAAA,EAAAS,IAAA;UACA,KAAAQ,UAAA;QACA;MACA;IACA;IACAA,WAAA;MACA,KAAAE,QAAA,CAAAC,OAAA;MACA,KAAAT,OAAA,MAAAP,SAAA;MACA,KAAAC,OAAA;IACA;IACAgB,WAAAR,EAAA;MACAjB,GAAA,CAAAyB,UAAA,CAAAR,EAAA,EAAAJ,IAAA;QACA,KAAAU,QAAA,CAAAC,OAAA;QACA,KAAAT,OAAA,MAAAP,SAAA;MACA;IACA;EACA;EACAkB,QAAA;IACA,KAAAf,KAAA;EACA;AACA","ignoreList":[]}]}