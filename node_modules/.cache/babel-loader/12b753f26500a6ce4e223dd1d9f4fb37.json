      this.queryForm.studentName\n        )\n        .then(res => {\n          this.tableData = res;\n        });\n    },\n    edit(id) {\n      api.get(id).then(res => {\n        this.entityForm = res;\n        this.editing = true;\n      });\n    },\n    save() {\n      api.update(this.entityForm).then(() => {\n        this.$message.success(\"成功\");\n        this.getPage(this.pageIndex);\n        this.editing = false;\n      });\n    }\n  },\n  created() {\n    this.query();\n  }\n};\n</script>\n\n<style scoped></style>\n"],"mappings":"AAiGA,YAAAA,GAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;QACAC,UAAA;QACAC,WAAA;MACA;MACAC,UAAA;MACAC,SAAA;MACAC,QAAA,EAAAR,GAAA,CAAAQ,QAAA;MACAC,SAAA;MACAC,SAAA;MACAC,OAAA;IACA;EACA;EACAC,OAAA;IACAC,MAAA;MACAb,GAAA,CACAc,YAAA,MAAAX,SAAA,CAAAC,UAAA,OAAAD,SAAA,CAAAE,WAAA,EACAU,IAAA,CAAAC,GAAA;QACA,KAAAP,SAAA,GAAAO,GAAA;QACA,KAAAN,SAAA;QACA,KAAAO,OAAA;MACA;IACA;IACAA,QAAAP,SAAA;MACAV,GAAA,CACAiB,OAAA,CACAP,SAAA,EACA,KAAAP,SAAA,CAAAC,UAAA,EACA,KAAAD,SAAA,CAAAE,WACA,EACAU,IAAA,CAAAC,GAAA;QACA,KAAAT,SAAA,GAAAS,GAAA;MACA;IACA;IACAE,KAAAC,EAAA;MACAnB,GAAA,CAAAoB,GAAA,CAAAD,EAAA,EAAAJ,IAAA,CAAAC,GAAA;QACA,KAAAV,UAAA,GAAAU,GAAA;QACA,KAAAL,OAAA;MACA;IACA;IACAU,KAAA;MACArB,GAAA,CAAAsB,MAAA,MAAAhB,UAAA,EAAAS,IAAA;QACA,KAAAQ,QAAA,CAAAC,OAAA;QACA,KAAAP,OAAA,MAAAP,SAAA;QACA,KAAAC,OAAA;MACA;IACA;EACA;EACAc,QAAA;IACA,KAAAZ,KAAA;EACA;AACA","ignoreList":[]}]}